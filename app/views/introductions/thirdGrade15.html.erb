
<h3 class="text-center " style="margin-top: 30px;margin-bottom:20px;">7기 - 15학번</h3>
<div class="d-flex justify-content-around align-items-around grade">
        <% @thirdGrade15.each do |student| %>
            <div class="grade-picture">
                <%= image_tag "/assets/thirdGrade15/#{student}" ,class: ""%>
                <%= content_tag(:p, "#{student.split('.').first.split(' ').last}", class: ["text-muted", "text-center"])%>  
            </div>
        <% end %>
</div>