<div class="container my-4">
  <%= form_for(@alumnusfee, html: { multipart: true }) do |h| %>
    <%= h.hidden_field :user_id, value: current_user.id %>
    <div class="form-group">
      <%= h.label :accounts, value: "회계자료" %>
      <%= h.file_field(:accounts, multiple: true,class: 'form-control') %>
    </div>
    <div class="form-group">
      <%= h.label :title, value: "제목" %>
      <%= h.text_field :title, class: "form-control", placeholder: "ㅁㅁ년 ㅁ월 동창회비 납부" %>
    </div>
    <div class="form-group">
      <%= h.label :content, value: "내용" %>
      <%= h.text_area :content, class: "tinymce", :rows => 15, :cols => 120 %>
      <%= tinymce %>
    </div>
    
    <%= button_to "게시하기", alumnusfees_path, method: :post, class: "btn btn-primary" %>
  <% end %>
</div>